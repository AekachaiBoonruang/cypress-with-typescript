{
  "name": "cypress-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "merge:json": "yarn --silent mochawesome-merge ./cypress/results/report/*.json > ./cypress/results/mochawesome.json",
    "generate:report": "yarn --silent marge ./cypress/results/mochawesome.json -o ./cypress/results && rm -rf cypress/results/report",
    "move:report": "mv cypress/results/mochawesome.html cypress/results/index.html",
    "report": "yarn merge:json && yarn generate:report && yarn move:report"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "cypress": "^6.4.0",
    "webpack": "^5.21.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "cypress-multi-reporters": "^1.4.0",
    "mocha-junit-reporter": "^2.0.0",
    "mochawesome": "^6.2.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.1.0",
    "typescript": "^4.1.4"
  }
}
